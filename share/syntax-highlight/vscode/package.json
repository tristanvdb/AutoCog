{
  "name": "stl-language",
  "displayName": "STL Language Support",
  "description": "Syntax highlighting for STL",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [{
      "id": "stl",
      "aliases": ["STL", "stl"],
      "extensions": [".stl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "stl",
      "scopeName": "source.stl",
      "path": "./syntaxes/stl.tmLanguage.json"
    }]
  }
}
